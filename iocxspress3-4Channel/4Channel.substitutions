# Macros:
#  P        Device Prefix
#  R        Device Suffix
#  PORT     Asyn Port name
#  TIMEOUT  Timeout
#  ADDR     Asyn Port address
file $(ADCORE)/db/NDROI.template
{
pattern { P, R, PORT, TIMEOUT, ADDR }
    { "$(PREFIX):", "ROI1:", "$(PORT).ROI1", "1", "0" }
    { "$(PREFIX):", "ROISUM1:", "$(PORT).ROISUM1", "1", "0" }
    { "$(PREFIX):", "ROI2:", "$(PORT).ROI2", "1", "0" }
    { "$(PREFIX):", "ROISUM2:", "$(PORT).ROISUM2", "1", "0" }
    { "$(PREFIX):", "ROI3:", "$(PORT).ROI3", "1", "0" }
    { "$(PREFIX):", "ROISUM3:", "$(PORT).ROISUM3", "1", "0" }
    { "$(PREFIX):", "ROI4:", "$(PORT).ROI4", "1", "0" }
    { "$(PREFIX):", "ROISUM4:", "$(PORT).ROISUM4", "1", "0" }
    { "$(PREFIX):", "ROIDATA:", "$(PORT).ROIDATA", "1", "0" }
}

# Macros:
#  P          Device Prefix
#  R          Device Suffix
#  PORT       Asyn Port name
#  TIMEOUT    Timeout
#  ADDR       Asyn Port address
#  TYPE       Asyn Type e.g. Int32
#  FTVL       Format, e.g. Int
#  NELEMENTS  Number of elements
file $(ADCORE)/db/NDStdArrays.template
{
pattern { P, R, PORT, TIMEOUT, ADDR, TYPE, FTVL, NELEMENTS }
    { "$(PREFIX):", "ARR1:", "$(PORT).ARR1", "1", "0", "Float64", "DOUBLE", "$(XSIZE)" }
    { "$(PREFIX):", "ARRSUM1:", "$(PORT).ARRSUM1", "1", "0", "Float64", "DOUBLE", "$(XSIZE)" }
    { "$(PREFIX):", "ARR2:", "$(PORT).ARR2", "1", "0", "Float64", "DOUBLE", "$(XSIZE)" }
    { "$(PREFIX):", "ARRSUM2:", "$(PORT).ARRSUM2", "1", "0", "Float64", "DOUBLE", "$(XSIZE)" }
    { "$(PREFIX):", "ARR3:", "$(PORT).ARR3", "1", "0", "Float64", "DOUBLE", "$(XSIZE)" }
    { "$(PREFIX):", "ARRSUM3:", "$(PORT).ARRSUM3", "1", "0", "Float64", "DOUBLE", "$(XSIZE)" }
    { "$(PREFIX):", "ARR4:", "$(PORT).ARR4", "1", "0", "Float64", "DOUBLE", "$(XSIZE)" }
    { "$(PREFIX):", "ARRSUM4:", "$(PORT).ARRSUM4", "1", "0", "Float64", "DOUBLE", "$(XSIZE)" }
}

# Macros:
#  P        Device Prefix
#  R        Device Suffix
#  PORT     Asyn Port name
#  TIMEOUT  Timeout
#  ADDR     Asyn Port address
file $(ADCORE)/db/NDProcess.template
{
pattern { P, R, PORT, TIMEOUT, ADDR }
    { "$(PREFIX):", "PROC:", "$(PORT).PROC", "1", "0" }
}

# Macros:
#  P        Device Prefix
#  R        Device Suffix
#  PORT     Asyn Port name
#  TIMEOUT  Timeout
#  ADDR     Asyn Port address
file $(ADCORE)/db/NDFile.template
{
pattern { P, R, PORT, TIMEOUT, ADDR }
    { "$(PREFIX):", "HDF5:", "$(PORT).HDF5", "1", "0" }
}

# Macros:
#  P     Device Prefix
#  R     Device Suffix
#  PORT  Asyn Port name
file $(ADCORE)/db/NDFileHDF5.template
{
pattern { P, R, PORT }
    { "$(PREFIX):", "HDF5:", "$(PORT).HDF5" }
}

# Macros:
#  P     Device prefix
#  R     Device suffix
#  HDF   HDF plugin record prefix
#  PROC  Processing plugin record prefix
file $(XSPRESS3)/db/highlevel.template
{
pattern { P, R, HDF, PROC }
    { "$(PREFIX)", "det1:", "$(PREFIX)HDF5:", "$(PREFIX)PROC:" }
}

# Macros:
#  IOCNAME  Device prefix
#  name     Object name and GUI association name
file $(DEVIOCSTATS)/db/ioc.db
{
pattern { IOCNAME, name }
    { "$(PREFIX)", "" }
}
