    { "$(PREFIX):", "C3_ROI14:", "$(PORT).C3_ROI14", "0", "1", "$(XSIZE)" }
    { "$(PREFIX):", "C3_ROI15:", "$(PORT).C3_ROI15", "0", "1", "$(XSIZE)" }
    { "$(PREFIX):", "C3_ROI16:", "$(PORT).C3_ROI16", "0", "1", "$(XSIZE)" }
    { "$(PREFIX):", "C4_SCA0:", "$(PORT).C4_SCA0", "0", "1", "$(XSIZE)" }
    { "$(PREFIX):", "C4_SCA1:", "$(PORT).C4_SCA1", "0", "1", "$(XSIZE)" }
    { "$(PREFIX):", "C4_SCA2:", "$(PORT).C4_SCA2", "0", "1", "$(XSIZE)" }
    { "$(PREFIX):", "C4_SCA3:", "$(PORT).C4_SCA3", "0", "1", "$(XSIZE)" }
    { "$(PREFIX):", "C4_SCA4:", "$(PORT).C4_SCA4", "0", "1", "$(XSIZE)" }
    { "$(PREFIX):", "C4_SCA5:", "$(PORT).C4_SCA5", "0", "1", "$(XSIZE)" }
