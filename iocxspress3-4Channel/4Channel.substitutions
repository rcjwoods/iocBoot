    { "$(PREFIX):", "C4_SCA7:", "$(PORT).C4_SCA7", "0", "1", "$(XSIZE)" }
    { "$(PREFIX):", "C4_ROI1:", "$(PORT).C4_ROI1", "0", "1", "$(XSIZE)" }
    { "$(PREFIX):", "C4_ROI2:", "$(PORT).C4_ROI2", "0", "1", "$(XSIZE)" }
    { "$(PREFIX):", "C4_ROI3:", "$(PORT).C4_ROI3", "0", "1", "$(XSIZE)" }
    { "$(PREFIX):", "C4_ROI4:", "$(PORT).C4_ROI4", "0", "1", "$(XSIZE)" }
    { "$(PREFIX):", "C4_ROI5:", "$(PORT).C4_ROI5", "0", "1", "$(XSIZE)" }
